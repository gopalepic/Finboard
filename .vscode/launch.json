{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run npm dev",
      "request": "launch",
      "type": "node-terminal",
      "command": "npm run dev"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/modules/stocks/stocks.service.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}   
